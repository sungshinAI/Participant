<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TwoActivity">

    <TextView
        android:id="@+id/tv10_1"
        android:layout_width="246dp"
        android:layout_height="50dp"
        android:autoSizeTextType="uniform"
        android:gravity="center"
        android:text="We Meet Here 회의"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="96dp" />

    <Button
        android:id="@+id/btn10_1"
        android:layout_width="281dp"
        android:layout_height="42dp"
        android:background="@drawable/buttonround"
        android:backgroundTint="#651463FC"
        android:gravity="center"
        android:text="가능"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="279dp" />

    <Button
        android:id="@+id/btn10_3"
        android:layout_width="320dp"
        android:layout_height="45dp"
        android:background="@drawable/buttonround"
        android:backgroundTint="#1463fc"
        android:gravity="center"
        android:text="다음"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="640dp" />

    <Button
        android:id="@+id/btn10_2"
        android:layout_width="281dp"
        android:layout_height="42dp"
        android:background="@drawable/buttonround"
        android:backgroundTint="#D9D9D9"
        android:gravity="center"
        android:text="불가능"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="416dp" />

</androidx.constraintlayout.widget.ConstraintLayout>